{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/**",
          "attached_assets/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/vercel.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/server/vercel.ts"
    }
  ],
  "outputDirectory": "dist/public"
}